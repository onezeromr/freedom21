{
  "expo": {
    "name": "Freedom21 - Wealth Calculator",
    "slug": "freedom21-wealth-calculator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "freedom21",
    "userInterfaceStyle": "dark",
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router", 
      "expo-font", 
      "expo-web-browser",
      [
        "expo-router",
        {
          "origin": "https://freedom21.me/"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}